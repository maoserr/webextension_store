  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^27.2.5",
    "prettier": "2.8.4",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.3"
